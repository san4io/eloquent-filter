{
    "name": "san4io/eloquent-filter",
    "description": "Laravel Eloquent Filter Package",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*"
    },
    "author": {
        "name": "Aleksandr Golubovskij",
        "email": "alegol90@gmail.com"
    },
    "autoload": {
        "psr-4": {
            "San4io\\EloquentFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "San4io\\EloquentFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "San4io\\EloquentFilter\\EloquentFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "~3.0"
    }
}
